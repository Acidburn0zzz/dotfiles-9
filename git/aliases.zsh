# Git
alias gb="git branch"
alias grv="git remote -v"
alias gs="git status -sb"
alias gst="git status"
alias gd="git diff"
alias gco="git checkout"
alias gcom="git checkout master"
alias gf="git fetch"
alias gl="git log"
alias ga="git add"
alias gc="git commit"
alias gp="git push"
alias gpgh="git push github master"
